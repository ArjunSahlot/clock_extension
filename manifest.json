{
    "name": "Clock",
    "version": "1.0",
    "manifest_version": 2,
    "icons": {
        "32": "images/icon.png"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["content.js"]
    }],
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Clock"
    }
}
